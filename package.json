{
  "name": "plone-vue",
  "version": "0.0.1",
  "license": "MIT",
  "description": "A simple Vue.js SDK to build web sites easily on top of the Plone RESTAPI",
  "author": "Kevin Bieri <k.bieri@4teamwork.ch>",
  "main": "dist/plone-vue.js",
  "module": "src/traverser/install.js",
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "npm run dev",
    "lint": "eslint --ext .js,.vue src",
    "unit": "jest test/unit --setupTestFrameworkScriptFile='<rootDir>/test/unit/setup.js'",
    "unit-dev": "jest test/unit --watch  --setupTestFrameworkScriptFile='<rootDir>/test/unit/setup.js'",
    "e2e": "node test/e2e/runner.js",
    "test": "yarn unit && yarn e2e",
    "build": "node build/build.js"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "join-path": "^1.1.1",
    "path.join": "^1.0.0",
    "url-parse": "^1.1.9"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.2",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^4.1.2",
    "chalk": "^2.0.1",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^21.2.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.4",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.3",
    "jest": "^21.2.1",
    "jest-vue-preprocessor": "^1.3.1",
    "jsdom": "^11.3.0",
    "moxios": "^0.4.0",
    "node-mock-server": "^0.18.2",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "ora": "^1.2.0",
    "portfinder": "^1.0.13",
    "puppeteer": "^0.13.0",
    "rimraf": "^2.6.0",
    "semver": "^5.3.0",
    "shelljs": "^0.7.6",
    "spawn-args": "^0.2.0",
    "url-loader": "^0.5.8",
    "vue": "^2.5.2",
    "vue-loader": "^13.3.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.2",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.5.2",
    "vue-test-utils": "^1.0.0-beta.2",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleDirectories": [
      "<rootDir>/node_modules",
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "^@[/](.+)": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
